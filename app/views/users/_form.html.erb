<%= form_with(model: @user, local: true) do |form| %>

  <div class="field">
    <%= form.label "Email:" %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :password, "Password:" %>
    <%= form.password_field :password %>
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <% end %>





