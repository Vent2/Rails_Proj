<p id="notice"><%= notice %></p>

<h1>Raves</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @raves.each do |r| %>
      <tr>
        <td><%= r.name %></td>
        <td><%= r.price %></td>
        <td><%= r.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Main Page', user_path(@user) %>
